{
  "expo": {
    "name": "dev-app",
    "slug": "dev-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/meau-icone.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barreirosm.devapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/meau-icone.png",
        "backgroundColor": "#ffffff",
        "imageWidth": 200
      },
      "package": "com.barreirosm.devapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/meau-icone.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffd358"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c6d75c08-b266-4ca1-9e7a-238212c5acda"
      }
    }
  }
}
